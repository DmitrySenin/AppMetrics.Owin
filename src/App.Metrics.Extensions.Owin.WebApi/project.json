{
  "authors": [ "Allan Hardy" ],
  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "debugType": "full"
  },
  "dependencies": {
    "App.Metrics": "1.0.0-beta4-build00154",
    "App.Metrics.Extensions.Owin": "1.0.0-alpha1",
    "Microsoft.AspNet.WebApi.Core": "5.2.3",
    "Microsoft.AspNet.WebApi.Owin": "5.2.3",
    "Microsoft.Extensions.DependencyInjection": "1.1.0",
