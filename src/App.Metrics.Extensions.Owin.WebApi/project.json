    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.Extensions.PlatformAbstractions": "1.1.0",
    "Microsoft.Owin.Host.SystemWeb": "3.0.1",
    "NETStandard.Library": "1.6.1",
    "Owin": "1.0.0",
    "System.Reflection": "4.0.0.0"
  },
  "description": "Extends the App.Metrics library providing a set OWIN Middleware Components and Web Api extensions which will record things like error rates per endpoint and an overall rate, request rates per endpoint and an overall rate etc.",
